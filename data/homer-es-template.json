{
  "template": "homer-*",
    "mappings" : {
    "_default_" : {
       "_all" : {"enabled" : true, "omit_norms" : true},
       "dynamic_templates" : [ {
             "string_fields" : {
               "match" : "*",
               "match_mapping_type" : "string",
               "mapping" : {
                 "type" : "string", "index" : "analyzed", "omit_norms" : true,
                   "fields" : { "raw" : {"type": "string", "index" : "not_analyzed", "ignore_above" : 256}}
               }
             }
       }, {
          "geo_fields" : { "mapping": { "type": "geo_point" }, "match" : "geopoint" }
       }, {
          "ts_fields" : { "mapping" : { "type" : "date" }, "match" : "timestamp" }
       }, {
          "ip_fields" : { "mapping" : { "type" : "ip" }, "match" : "ip" }
       }, {
          "byte_fields" : { "mapping" : { "type" : "byte" }, "match" : "bytes" }
       } ]
       }
    }
}
